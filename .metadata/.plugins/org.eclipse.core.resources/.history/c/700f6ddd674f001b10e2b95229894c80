package com.ces.service.impl;

import java.awt.print.Book;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

import com.ces.model.BookModel;
import com.ces.service.BookService;

public class BookServiceImpl implements BookService{
	static List<BookModel> books = new ArrayList<>(Arrays.asList(new BookModel("Harry Potter", "Join Thame"),
			new BookModel("This summer", "Jack Love"), new BookModel("Family", "Black Back")));

	@Override
	public List<BookModel> findAllBooks() {
		return books;
	}
	
}
